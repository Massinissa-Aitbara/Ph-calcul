        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <EditText
        android:id="@+id/ed2"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginTop="7dp"
