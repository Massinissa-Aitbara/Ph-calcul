        android:inputType="numberDecimal"
        android:maxLength="8"
        android:padding="12dp"
        android:textSize="15dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.646"
